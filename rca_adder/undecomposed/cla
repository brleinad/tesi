#! /usr/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x25721f0 .scope module, "tb_cla_adder" "tb_cla_adder" 2 3;
 .timescale -9 -11;
P_0x2572370 .param/l "NBIT" 0 3 1, +C4<00000000000000000000000000001000>;
v0x2598aa0_0 .var "a", 7 0;
v0x2598b60_0 .var "b", 7 0;
v0x2598c30_0 .net "s", 7 0, v0x2598940_0;  1 drivers
S_0x2572440 .scope module, "DUT" "cla_adder" 2 11, 4 1 0, S_0x25721f0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "a"
    .port_info 1 /INPUT 8 "b"
    .port_info 2 /OUTPUT 8 "s"
P_0x2572670 .param/l "NBIT" 0 3 1, +C4<00000000000000000000000000001000>;
v0x25759d0_0 .net "a", 7 0, v0x2598aa0_0;  1 drivers
v0x2598480_0 .net "b", 7 0, v0x2598b60_0;  1 drivers
v0x2598560_0 .var "c", 7 0;
v0x2598650_0 .var "g", 7 0;
v0x2598730_0 .var/i "i", 31 0;
v0x2598860_0 .var "p", 7 0;
v0x2598940_0 .var "s", 7 0;
E_0x2577610 .event edge, v0x2598480_0, v0x25759d0_0;
    .scope S_0x2572440;
T_0 ;
    %wait E_0x2577610;
    %load/vec4 v0x25759d0_0;
    %load/vec4 v0x2598480_0;
    %and;
    %store/vec4 v0x2598650_0, 0, 8;
    %load/vec4 v0x25759d0_0;
    %load/vec4 v0x2598480_0;
    %xor;
    %store/vec4 v0x2598860_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x2598560_0, 4, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x2598730_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x2598730_0;
    %cmpi/s 7, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x2598650_0;
    %load/vec4 v0x2598730_0;
    %part/s 1;
    %load/vec4 v0x2598860_0;
    %load/vec4 v0x2598730_0;
    %part/s 1;
    %load/vec4 v0x2598560_0;
    %load/vec4 v0x2598730_0;
    %part/s 1;
    %and;
    %or;
    %load/vec4 v0x2598730_0;
    %addi 1, 0, 32;
    %ix/vec4/s 4;
    %store/vec4 v0x2598560_0, 4, 1;
    %load/vec4 v0x2598730_0;
    %addi 1, 0, 32;
    %store/vec4 v0x2598730_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x2598860_0;
    %load/vec4 v0x2598560_0;
    %xor;
    %store/vec4 v0x2598940_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x25721f0;
T_1 ;
    %vpi_call 2 21 "$monitor", "A:%d B:%d S:%d ", v0x2598aa0_0, v0x2598b60_0, v0x2598c30_0 {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x2598aa0_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x2598b60_0, 0, 8;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x2598aa0_0, 0, 8;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x2598b60_0, 0, 8;
    %delay 1000, 0;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x2598aa0_0, 0, 8;
    %delay 500, 0;
    %pushi/vec4 11, 0, 8;
    %store/vec4 v0x2598b60_0, 0, 8;
    %delay 500, 0;
    %pushi/vec4 55, 0, 8;
    %store/vec4 v0x2598aa0_0, 0, 8;
    %delay 500, 0;
    %pushi/vec4 110, 0, 8;
    %store/vec4 v0x2598b60_0, 0, 8;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 5;

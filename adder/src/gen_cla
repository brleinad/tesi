#! /usr/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x13899d0 .scope module, "tb_cla_adder" "tb_cla_adder" 2 3;
 .timescale -9 -11;
P_0x1389b50 .param/l "NBIT" 0 2 5, +C4<00000000000000000000000000000100>;
v0x13b2170_0 .var "a", 3 0;
v0x13b2280_0 .var "b", 3 0;
v0x13b2350_0 .var "cin", 0 0;
v0x13b2450_0 .net "cout", 0 0, v0x13b1bb0_0;  1 drivers
v0x13b2520_0 .net "s", 3 0, v0x13b1f60_0;  1 drivers
S_0x1389bf0 .scope module, "DUT" "cla_adder" 2 14, 3 1 0, S_0x13899d0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a"
    .port_info 1 /INPUT 4 "b"
    .port_info 2 /INPUT 1 "cin"
    .port_info 3 /OUTPUT 4 "s"
    .port_info 4 /OUTPUT 1 "cout"
P_0x1389d70 .param/l "NBIT" 0 3 5, +C4<00000000000000000000000000000100>;
v0x138e960_0 .net "a", 3 0, v0x13b2170_0;  1 drivers
v0x13b1920_0 .net "b", 3 0, v0x13b2280_0;  1 drivers
v0x13b1a00_0 .var "c", 4 0;
v0x13b1af0_0 .net "cin", 0 0, v0x13b2350_0;  1 drivers
v0x13b1bb0_0 .var "cout", 0 0;
v0x13b1cc0_0 .var "g", 3 0;
v0x13b1da0_0 .var/i "i", 31 0;
v0x13b1e80_0 .var "p", 3 0;
v0x13b1f60_0 .var "s", 3 0;
E_0x138a360 .event edge, v0x13b1af0_0, v0x13b1920_0, v0x138e960_0;
    .scope S_0x1389bf0;
T_0 ;
    %wait E_0x138a360;
    %load/vec4 v0x138e960_0;
    %load/vec4 v0x13b1920_0;
    %and;
    %store/vec4 v0x13b1cc0_0, 0, 4;
    %load/vec4 v0x138e960_0;
    %load/vec4 v0x13b1920_0;
    %xor;
    %store/vec4 v0x13b1e80_0, 0, 4;
    %load/vec4 v0x13b1af0_0;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x13b1a00_0, 4, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x13b1da0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x13b1da0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x13b1cc0_0;
    %load/vec4 v0x13b1da0_0;
    %part/s 1;
    %load/vec4 v0x13b1e80_0;
    %load/vec4 v0x13b1da0_0;
    %part/s 1;
    %load/vec4 v0x13b1a00_0;
    %load/vec4 v0x13b1da0_0;
    %part/s 1;
    %and;
    %or;
    %load/vec4 v0x13b1da0_0;
    %addi 1, 0, 32;
    %ix/vec4/s 4;
    %store/vec4 v0x13b1a00_0, 4, 1;
    %load/vec4 v0x13b1da0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x13b1da0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x13b1a00_0;
    %parti/s 1, 4, 4;
    %store/vec4 v0x13b1bb0_0, 0, 1;
    %load/vec4 v0x13b1e80_0;
    %load/vec4 v0x13b1a00_0;
    %parti/s 4, 0, 2;
    %xor;
    %store/vec4 v0x13b1f60_0, 0, 4;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x13899d0;
T_1 ;
    %vpi_call 2 24 "$monitor", "A:%d B:%d S:%d Cout:%d", v0x13b2170_0, v0x13b2280_0, v0x13b2520_0, v0x13b2450_0 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x13b2170_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x13b2280_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13b2350_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x13b2170_0, 0, 4;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x13b2280_0, 0, 4;
    %delay 1000, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x13b2170_0, 0, 4;
    %delay 500, 0;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x13b2280_0, 0, 4;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.

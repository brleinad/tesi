.design_name normal_adder
.inputnames  a[2] a[1] a[0] b[2] b[1] b[0]
.outputnames s[3] s[2] s[1] s[0]
s[3] = ((n9' * n10') + (b[2] * a[2])) ;
s[2] = (n10 ^ n9) ;
s[1] = (n11 ^ n12) ;
s[0] = (a[0] ^ b[0]) ;
n9 = ((b[1]' + a[1]') * (n11' + n12')) ;
n10 = (a[2] ^ b[2])' ;
n11 = (a[1] ^ b[1]) ;
n12 = (b[0] * a[0]) ;
